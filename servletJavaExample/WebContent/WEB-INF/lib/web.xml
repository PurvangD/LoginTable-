<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>servletJavaExample</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
<servlet> 
<servlet-name> testing </servlet-name>
<servlet-class> servletExample.loginServletExampleClass </servlet-class>
</servlet>

<servlet-mapping>
<servlet-name> testing </servlet-name>
<url-pattern> /testing.htm </url-pattern>
</servlet-mapping>



<servlet>
<servlet-name> Registration </servlet-name>
<servlet-class> servletExample.RegistrationServlet </servlet-class>
 </servlet>

<servlet-mapping>
<servlet-name> Registration </servlet-name>
<url-pattern> /RegistrationServlet.htm </url-pattern>	
</servlet-mapping>

</web-app>